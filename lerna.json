{
  "version": "0.1.0",
  "npmClient": "pnpm",
  "changelog": {
    "repo": "sphereon-opensource/oid4vc-demo",
    "cacheDir": ".changelog"
  },
  "command": {
    "publish": {
      "allowBranch": [
        "main",
        "next",
        "master",
        "unstable"
      ],
      "conventionalCommits": true,
      "gitRemote": "origin",
      "message": "chore(release): %s [skip ci]"
    }
  },
  "useWorkspaces": true
}
