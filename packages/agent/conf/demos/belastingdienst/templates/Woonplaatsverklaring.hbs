{
    "@context": [
        "https://raw.githubusercontent.com/Sphereon-Opensource/vc-contexts/master/e-invoicing/woonplaatsverklaring.schema.json",
        "https://www.w3.org/2018/credentials/v1", {
            "description": {
                "@id": "http://schema.org/description",
                "@container": "@language"
            }
        }
    ],
    "type": [
        "VerifiableCredential",
        "Woonplaatsverklaring"
    ],
    "expirationDate": "{{{ dateTimeAfterMonths 12 }}}",
    "credentialSubject": {
        "naam": "{{naam}}",
        "adres": {
            "straatnaam": "{{straatnaam}}",
            "huisnummer": "{{huisnummer}}",
            "postcode": "{{postcode}}",
            "plaats": "{{plaats}}"
        }
    }
}
