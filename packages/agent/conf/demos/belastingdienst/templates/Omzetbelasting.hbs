{
    "@context": [
        "https://raw.githubusercontent.com/Sphereon-Opensource/vc-contexts/master/e-invoicing/omzetbelasting.schema.json",
        "https://www.w3.org/2018/credentials/v1", {
            "description": {
                "@id": "http://schema.org/description",
                "@container": "@language"
            }
        }
    ],
    "type": [
        "VerifiableCredential",
        "Omzetbelasting"
    ],
    "expirationDate": "{{{ dateTimeAfterMonths 12 }}}",
    "credentialSubject": {
        "naam": "{{naam}}",
        "rsin": "{{rsin}}",
        "btw-id": "{{btwId}}",
        "ob-nummer": "{{obNummer}}"
    }
}
