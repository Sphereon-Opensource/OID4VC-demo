{
    "@context": [
        "https://www.w3.org/2018/credentials/v1", {
            "description": {
                "@id": "http://schema.org/description",
                "@container": "@language"
            }
        }
    ],
    "type": [
        "VerifiableCredential",
        "NotaryCompanyVerification"
    ],
    "expirationDate": "{{{ dateTimeAfterMonths 12 }}}",
    "credentialSubject": {
        "description": {
          "en": "Company has been verified by the notary",
          "nl": "Bedrijf is geverifieerd door de notaris"
         }
        {{{mergeJSON @root}}}
    }
}
