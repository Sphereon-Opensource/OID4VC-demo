  "sequencer": {
    "steps": [
      {
        "id": "verifyRequest",
        "operation": "navigate",
        "path": "/credentials/verify/request",
        "nextId": "infoRequest",
        "isDefaultRoute": true
      },
      {
        "id": "infoRequest",
        "operation": "navigate",
        "path": "/information/request",
        "nextId": "credentialOffer"
      },
      {
        "id": "credentialOffer",
        "operation": "execute",
        "action": "create-credential-offer",
        "nextId": "issueRequest"
      },
      {
        "id": "issueRequest",
        "operation": "navigate",
        "path": "/credentials/issue/request",
        "nextId": "issueSuccess"
      },
      {
        "id": "issueSuccess",
        "operation": "navigate",
        "path": "/credentials/issue/success"
      }
    ]
  },
