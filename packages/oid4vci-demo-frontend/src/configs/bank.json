          "operation": "navigate",
          "path": "/information/manual/request",
          "nextId": "credentialOffer"
        },
        {
          "id": "credentialOffer",
          "operation": "execute",
          "action": "create-credential-offer",
          "actionParams": {
            "issueCredentialType": "BankAccount"
          },
          "nextId": "issueRequest"
        },
        {
          "id": "issueRequest",
          "operation": "navigate",
          "path": "/credentials/issue/request",
          "nextId": "issueSuccess"
        },
        {
          "id": "issueSuccess",
          "operation": "navigate",
          "path": "https://knb.demo.sphereon.com"
        },
        {
          "id": "download",
          "operation": "navigate",
          "path": "/download"
        }
      ]
    }
