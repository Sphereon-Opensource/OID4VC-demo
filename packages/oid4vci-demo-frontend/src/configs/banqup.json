  "sequencer": {
    "steps": [
      {
        "id": "downloadSphereonWallet",
        "operation": "navigate",
        "path": "/download/sphereon-wallet",
        "nextId": "verifyCredential",
        "isDefaultRoute": true
      },
      {
        "id": "verifyCredential",
        "operation": "navigate",
        "path": "/credentials/verify/request",
        "nextId": "infoRequest"
      },
      {
        "id": "infoRequest",
        "operation": "navigate",
        "path": "/information/request",
        "nextId": "credentialOffer"
      },
      {
        "id": "credentialOffer",
        "operation": "execute",
        "action": "create-credential-offer",
        "nextId": "issueRequest"
      },
      {
        "id": "issueRequest",
        "operation": "navigate",
        "path": "/credentials/issue/request",
        "nextId": "issueSuccess"
      },
      {
        "id": "issueSuccess",
        "operation": "navigate",
        "path": "https://knb.demo.sphereon.com"
      }
    ]
  },
