      "@sphereon/ssi-types": "0.13.0",
      "@sphereon/ssi-sdk.core": "0.13.0",
      "@sphereon/ssi-sdk.data-store": "0.13.0",
      "@sphereon/ssi-sdk-ext.key-utils": "0.12.1",
      "@sphereon/ssi-sdk-ext.did-utils": "0.12.1",
      "@veramo/core": "4.2.0",
