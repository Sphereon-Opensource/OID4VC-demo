      "@sphereon/did-auth-siop": "0.3.2-unstable.11",
      "@sphereon/oid4vci-client": "0.7.1-next.18",
      "@sphereon/oid4vci-common": "0.7.1-next.18",
      "@sphereon/oid4vci-issuer": "0.7.1-next.18",
      "@sphereon/oid4vci-issuer-server": "0.7.1-next.18",
      "@veramo/core": "4.2.0",
